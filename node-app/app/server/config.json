  "global": {
    "env": "%NODE_ENV%",
    "app": {
      "title": "Dashboard for Labour",
      "description": "Application server for Dashboard for Labour.",
      "code": "d4l",
      "version": "0.8.0",
      "protocol": "http",
      "subdomain": "dashboard",
      "domain": "forlabour.com"
